#!groovy
properties([disableConcurrentBuilds()])

    pipeline {
        agent {
            label 'master'
        }
        options {
            buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
            timestamps()
        }
        stages {
            stage ("Compile aplication"){
                steps {
             
                    sh 'mvn -B -DskipTests clean package'
                }
                steps {
                    sh 'mvn -DskipTests compile package'
                }
            }
            stage('Testing') { 
                steps {
                    sh 'mvn test'
                }
            }
            stage('')


